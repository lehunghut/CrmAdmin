 {
    "name": "lehunghus/CrmAdmin",
    "description": "IdeaAdmin is a Open source Laravel Admin Panel / CMS which can be used as Admin Backend, Data Management Tool or CRM boilerplate for Laravel with features like CRUD Generation, Module Manager, Media, Menus, Backups and much more",
    "keywords": ["php", "laravel", "crm", "admin", "cms", "administrator"],
    "license": "MIT",
    "require": {
        "illuminate/support": "7.*",
        "laravelcollective/html": "^6.0",
        "illuminate/container": "^8.0",
        "yajra/laravel-datatables-oracle": "9.*",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "~2.10.2",
        "zizaco/entrust": "1.9.1",
        "spatie/laravel-backup": "^6.*"
    },
    "authors": [
        {
            "name": "Ganesh Bhosale",
            "email": "ganesh@idea.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Idea\\Ideaadmin\\": "src/"
        },
        "classmap": [
           "src/Controllers",
           "src/Models"
       ]
    }

}
